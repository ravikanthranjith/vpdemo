{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "visualpathdemo"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDatawarehouse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureDatawarehouse'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"EmpRepo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'EmpRepo'"
		},
		"KVVPDemo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'KVVPDemo'"
		},
		"LS_ASQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ASQL'"
		},
		"adlsTarget_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adlsTarget'"
		},
		"adlsTargetAug_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adlsTargetAug'"
		},
		"KVLinkedService_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KVDemo-VP.vault.azure.net/"
		},
		"adlsTarget_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://visualpathdemo.dfs.core.windows.net/"
		},
		"adlsTargetAug_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://visualpathdemo.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "visualpathdemo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDatawarehouse_connectionString": {
						"value": "[parameters('AzureDatawarehouse_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"EmpRepo_connectionString": {
						"value": "[parameters('EmpRepo_connectionString')]"
					},
					"LS_ASQL_connectionString": {
						"value": "[parameters('LS_ASQL_connectionString')]"
					},
					"adlsTarget_accountKey": {
						"value": "[parameters('adlsTarget_accountKey')]"
					},
					"adlsTargetAug_accountKey": {
						"value": "[parameters('adlsTargetAug_accountKey')]"
					},
					"KVLinkedService_properties_typeProperties_baseUrl": {
						"value": "[parameters('KVLinkedService_properties_typeProperties_baseUrl')]"
					},
					"adlsTarget_properties_typeProperties_url": {
						"value": "[parameters('adlsTarget_properties_typeProperties_url')]"
					},
					"adlsTargetAug_properties_typeProperties_url": {
						"value": "[parameters('adlsTargetAug_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "visualpathdemo_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"KVVPDemo_connectionString": {
						"value": "[parameters('KVVPDemo_connectionString')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'visualpathdemo_ArmTemplate_0')]"
			]
		},
		{
			"name": "visualpathdemo_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'visualpathdemo_ArmTemplate_1')]"
			]
		}
	]
}